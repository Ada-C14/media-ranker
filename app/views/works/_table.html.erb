<table class="table">
  <thead>
  <tr>
    <th>Title</th>
    <th>Created by</th>
    <th>Published</th>
    <th>Votes</th>
    <th>Upvote</th>
  </tr>
  </thead>

  <tbody>
  <% variable.each do |work| %>
    <tr>
      <td><%= link_to "#{work.title}", work_path(work.id) %></td>
      <td><%= "#{work.creator}" %></td>
      <td><%= "#{work.published}" %></td>
      <td><%= "Placeholder #{rand(10)}" %></td>
      <td><%= button_to "Placeholder"  %></td>
    </tr>
  <% end %>
  </tbody>
</table>