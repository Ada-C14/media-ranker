<h1>Votes#index</h1>
<p>Find me in app/views/votes/index.html.erb</p>

<table>
  <tr>
    <th>Vote ID</th>
    <th>User ID</th>
    <th>Work ID</th>

  </tr>
  <% @votes.each do |vote| %>
    <tr>
      <td>
        <%= vote.id %>
      </td>
      <td>
        <%= vote.user_id %>
      </td>
      <td>
        <%= vote.work_id %>
      </td>
    </tr>
  <%end %>
</table>