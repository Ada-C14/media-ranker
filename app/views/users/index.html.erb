<main>
<h2>List of Users</h2>
<table class="table">
  <thead>
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user_path(user) %></td>
      <td><%= user.votes.count %></td>
      <td><%= user.created_at.strftime("%B %d, %Y") %></td>
    </tr>
  <% end %>
  </tbody>
</table>
  <p><button class="btn btn-secondary"><%= link_to "Back to Media List", root_path %></button></p>
</main>
