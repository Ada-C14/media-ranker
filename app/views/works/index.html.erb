<h2> List of Works <h2>

<section class="media-table">
  <h4> Albums </h4>
  <table>
  <thead>
      <tr>
        <th>Votes</th>
        <th>Title</th>
        <th>Created By</th>
        <th>Published</th>
        <th>Upvote</th>
      </tr>
    </thead>
    <tbody>
      <% if @albums %>
        <% @albums.each do |album| %>
          <tr>
            <%#will have votes here%>
            <td><%= album.title %></td>
            <td><%= album.creator %></td>
            <td><%= album.publication_year %></td>
          </tr>
          <%#will have upvote button here%>
          <% end %>
    <% end %>
    </tbody>
  </table>
