<h1>List of Works</h1>
<ul>
  <% @works.each do |work| %>
    <li>
      <%= link_to work.title, work_path(work) %>
      <%= work.category %>
    </li>
  <% end %>
</ul>

<h1>Albums</h1>
<table>
  <thead>
  <tr>
    <th>Votes</th>
    <th>Title</th>
    <th>Created by</th>
    <th>Published</th>
    <th>Upvote</th>

  </tr>
  </thead>
</table>