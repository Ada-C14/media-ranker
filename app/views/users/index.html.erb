<h2>List of Users</h2>
<table id='users-table' class="table table-hover">
  <thead class="thead-light">
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.name, user_path(user.id)%></td>
      <td><%= rand(25) %></td>
      <td><%= user.created_at.strftime("%b %d, %Y") %></td>
    </tr>
  <% end %>
  </tbody>
</table>