<table>
  <tr>
    <th>Votes</th>
    <th>Title</th>
    <th>Created By</th>
    <th>Published</th>
    <th>Upvote</th>
  </tr>

  <% variable.each do |work| %>
    <tr>
      <td><%= work.votes.count %></td>
      <td><%= link_to work.title, work_path(work) %></td>
      <td><%= work.creator %></td>
      <td><%= work.publication_year %></td>
      <td><%= button_to "Upvote", action: "upvote", method: :post %></td>
    </tr>
  <% end %>
</table>