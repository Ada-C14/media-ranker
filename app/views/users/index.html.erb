<h2>List of Users</h2>

  <ul class="user-list">
    <% @users.each do |user| %>
        <li><b><%= link_to user.name.upcase, user_path(user.id) %></b> -- <%= user.created_at %> </li>
    <% end %>
  </ul>

<%= button_to "Back to media list", root_path, method: :get, form: {style: 'display:inline-block;'}, class: "btn btn-secondary" %>
