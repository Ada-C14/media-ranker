<h2>List of Users</h2>
<table class="table">
  <thead>
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
  </thead>
  <% @users.each do |user| %>
    <tbody>
    <tr>
      <td><%= link_to user.username, user_path(user) %></td>
      <td><%= user.votes.size %></td>
      <td><%= user.created_at.strftime('%b %d %Y') %></td>
    </tr>
    </tbody>
  <%end %>
</table>

<%#= button_to "See all Users", users_path, method: :get %>
<%= button_to "Back to media list", works_path, method: :get %>
