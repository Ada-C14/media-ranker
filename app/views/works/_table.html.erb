<h4> <%= work_title %></h4>
<table class="table">
<thead>
    <tr>
      <th>Votes</th>
      <th>Title</th>
      <th>Created By</th>
      <th>Published</th>
      <th>Upvote</th>
    </tr>
  </thead>
  <tbody>
    <% if works %>
      <% works.each do |work| %>
        <tr>
          <td><%= work.votes.length %></td>
          <td><%= link_to "#{work.title}", work_path(work.id)%></td>
          <td><%= work.creator %></td>
          <td><%= work.publication_year %></td>
          <td><%= button_to "Upvote", upvote_work_path(work.id) %></td>
        </tr>
        <% end %>
  <% end %>
  </tbody>
</table>